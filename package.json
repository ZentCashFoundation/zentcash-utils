{
  "name": "zentcash-utils",
  "version": "1.0.20",
  "description": "A Node.js collection of utilities useful for Zent Cash Developers",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "config.json",
    "index.d.ts",
    "dist/ZentCashUtils.js",
    "lib/*"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "standard && mocha tests/tests.js",
    "build-docs": "jsdoc -c jsdoc.json",
    "prepare": "webpack"
  },
  "keywords": [
    "ZentCash",
    "Utilities"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZentCashFoundation/zentcash-utils.git"
  },
  "author": "Zent Cash Developers",
  "contributors": [
    "Zent Cash Developers"
  ],
  "license": "GPL-3.0",
  "optionalDependencies": {
    "zentcash-crypto": "^3.1.4"
  },
  "dependencies": {
    "crc": "^3.8.0",
    "get-random-values": "^1.2.0",
    "numeral": "^2.0.6",
    "secure-random-string": "^1.1.0",
    "varint": "^5.0.0"
  },
  "standard": {
    "ignore": [
      "lib/zentcash-crypto/zentcash-crypto.js",
      "lib/zentcash-crypto/zentcash-crypto-wasm.js",
      "dist/*",
      "docs/*"
    ]
  },
  "devDependencies": {
    "mocha": "^6.2.2",
    "standard": "^12.0.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
